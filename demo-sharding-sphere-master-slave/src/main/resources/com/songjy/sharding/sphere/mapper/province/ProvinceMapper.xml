<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.songjy.sharding.sphere.mapper.province.ProvinceMapper">

  <insert id="save" parameterType="com.songjy.sharding.sphere.model.province.Province">
    INSERT INTO t_province(id, `name`) VALUES(#{id}, #{name})
  </insert>

  <select id="selectById" resultType="com.songjy.sharding.sphere.model.province.Province">
    SELECT * FROM `t_province` WHERE id = #{id,jdbcType=INTEGER}
  </select>

</mapper>